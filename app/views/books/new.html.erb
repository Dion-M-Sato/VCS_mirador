<%=  provide(:title, 'Request') %>
<h1>Request</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <%= form_for(@book) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.label :manifestURI %>
      <%= f.text_field :uri, class: 'form-control' %>
      <%= f.label :islocked? %>
      <%= f.check_box(:isLocked ,{},checked_value = "true",unchecked_value = "false" )%>
      <%= f.submit "Request", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>
<form name="geturl">
  Fetch URL: <input type="text" />
<input type="submit" id ="url" value="Get">
</form>
<script src="app/assets/javascripts/get_manifest_uri.js"></script>
<p>Please past your URL.</p>
